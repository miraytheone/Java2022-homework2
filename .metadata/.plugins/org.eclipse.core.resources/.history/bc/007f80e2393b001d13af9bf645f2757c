package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {
		int myList[] = {3,5,7,9,11,15,16,23};
		int remainder;
		for( int number:myList) {
			remainder = number % 2 ;
			if(remainder == 0) {
				System.out.println("Sayı asal değildir.");
			}
			else {
				System.out.println("Sayı asaldır.");
			}
		}
 
	}

}
